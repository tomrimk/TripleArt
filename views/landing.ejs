    <div id="demo" class="row demoRow">
      
        <h1>Išbandykite demo</h1>
        <% for(var i = 0; i < campaigns.length; i++) {%>
        <div class="col-lg-<%=12/campaigns.length%>">
              <div class="well">
                <img src="/images/demola.png" alt="">
                <h2><a href="/campaign/<%=campaigns[i]._id%>"><%= campaigns[i].title%></a></h2>
                <button type="button" class="btn btn-lg btn-primary">Registruotis</button>
                <h4>Sukūrė: <%= campaigns[i].autorius.vardas %></h4>
                <h4>Sukurta prieš: <%= moment(campaigns[i].sukurimoData).fromNow() %></h4>
              </div>
          </div>
        <%}%>
      
    </div>
    <div class="row roadMapDiv">
      <div class="col-lg-12 ">
        <h1>Roadmap</h1>
        <img src="/images/roadmap.jpg" class="img-responsive" alt="">
